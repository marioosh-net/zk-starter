<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<zk>
	<window title="Hello World!!" border="normal" width="500px" apply="net.marioosh.zkstarter.DemoWindowComposer">
		
		<div>DATE: ${springService.date}</div>
		<separator/>
		
		<textbox id="textbox"/>
		<button id="okButton" label="ok"/>
		<separator/>
		
		<div>USERS</div>
		<grid>
			<columns>
				<column>login</column>
				<column>firstname</column>
				<column>lasttname</column>
			</columns>
		   <rows>
		     <row forEach="${springService.users}">
		       <label value="${each.login}"/><label value="${each.firstname}"/><label value="${each.lastname}"/>
		     </row>
		   </rows>
		 </grid>
						
	</window>
</zk>